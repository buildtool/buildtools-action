{
  "name": "setup-buildtools",
  "version": "1.0.0",
  "private": true,
  "description": "setup buildtools action",
  "main": "lib/setup-buildtools.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "npm run format && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildtool/setup-buildtools-action.git"
  },
  "keywords": [
    "actions",
    "buildtools",
    "setup"
  ],
  "author": "Buildtools",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/http-client": "^2.0.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.1",
    "@types/semver": "^7.3.12",
    "@vercel/ncc": "^0.34.0",
    "jest": "^29.1.2",
    "jest-circus": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
